{
  "name": "frosit/magepatch",
  "description": "helps to find Magento patches and verify them",
  "type": "project",
  "homepage": "https://github.com/frosit/magepatch",
  "keywords": [
    "magento",
    "Patch",
    "patches",
    "console",
    "SUPEE",
    "finder"
  ],
  "license": "GPL-3.0",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Fabio Ros",
      "email": "fabio@frosit.nl",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "symfony/console": "^3.2",
    "symfony/yaml": "^3.2",
    "version/version": "^2.1",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.4",
    "phpspec/phpspec": "^4.0",
    "phpunit/phpunit": "^6.2",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Frosit\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "bin": [
    "bin/console"
  ]
}
