{
  "name": "frosit/magepatch",
  "description": "A CLI Tool that helps find/ download the right patches for your Magento store.",
  "homepage": "https://github.com/frosit/magepatch",
  "type": "project",
  "license": "GPL-3.0",
  "prefer-stable": true,
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/frosit/magepatch/issues"
  },
  "keywords": [
    "magento",
    "Patch",
    "patches",
    "console",
    "SUPEE",
    "finder"
  ],
  "authors": [
    {
      "name": "Fabio Ros",
      "email": "info@frosit.nl",
      "role": "Developer",
      "homepage": "http://fros.it"
    }
  ],
  "require": {
    "php": "^5.5",
    "symfony/console": "^3.2",
    "symfony/yaml": "^3.2",
    "version/version": "^2.1",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.4",
    "phpspec/phpspec": "^3.4",
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Frosit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Frosit\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "bin": [
    "bin/console"
  ]
}
